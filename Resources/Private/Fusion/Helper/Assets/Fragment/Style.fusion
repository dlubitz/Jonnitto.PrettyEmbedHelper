prototype(Jonnitto.PrettyEmbedHelper:Assets.Fragment.Style) < prototype(Neos.Fusion:Component) {
    filename = null
    uri = ${StaticResource.uri('Jonnitto.PrettyEmbedHelper', 'Public/Styles/' + this.filename)}

    renderer = afx`
        <link
            data-slipstream
            rel="stylesheet"
            href={props.uri}
        />
    `
}
