prototype(Jonnitto.PrettyEmbed:Assets.Fragment.Script) < prototype(Neos.Fusion:Component) {
    filename = null
    uri = ${StaticResource.uri('Jonnitto.PrettyEmbedHelper', 'Public/Scripts/' + this.filename)}
    slipstream = true

    renderer = afx`
        <script
            data-slipstream={props.slipstream}
            defer
            src={props.uri}
        ></script>
    `
}
