prototype(Jonnitto.PrettyEmbed:Assets.Fragment.Script) < prototype(Neos.Fusion:Component) {
    filename = null
    uri = ${StaticResource.uri('Jonnitto.PrettyEmbedHelper', 'Public/Script/' + this.filename)}

    renderer = afx`
        <script
            data-slipstream
            defer
            type="module"
            src={props.uri}
        ></script>
    `
}
