prototype(Jonnitto.PrettyEmbedHelper:Assets.Fragment.Module) < prototype(Neos.Fusion:Component) {
    filename = null
    uri = ${StaticResource.uri('Jonnitto.PrettyEmbedHelper', 'Public/Modules/' + this.filename)}

    renderer = afx`
        <script
            data-slipstream
            data-slipstream-prepend
            defer
            type="module"
            src={props.uri}
        ></script>
    `
}
